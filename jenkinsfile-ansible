pipeline {
    agent {label "docker-slave"}
    stages {
        stage('maven build'){
            steps{
                sh 'mvn -DskipTests clean install'
            }
        }
        stage('Deploy to Dev Server'){
            steps{
                sh 'cd /home/ubuntu/ansible/'
                sh "ansible-playbook tomcat-deploy-dev.yml"
            }
        }
        }
}
